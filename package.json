{
  "name": "abyssdb",
  "version": "1.0.2",
  "description": "Abyss DB toolkit (Prisma + seeds + soft-delete extensions)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib/client": {
      "types": "./dist/lib/client.d.ts",
      "default": "./dist/lib/client.js"
    },
    "./helpers/cleanUp": {
      "types": "./dist/helpers/cleanUp.d.ts",
      "default": "./dist/helpers/cleanUp.js"
    },
    "./extensions/softDelete": {
      "types": "./dist/extensions/softDelete.d.ts",
      "default": "./dist/extensions/softDelete.js"
    },
    "./prisma/generated/client": {
      "types": "./prisma/generated/client/index.d.ts",
      "default": "./prisma/generated/client/index.js"
    },
    "./prisma/schema.prisma": "./prisma/schema.prisma",
    "./user/seed": {
      "types": "./dist/user/seed.d.ts",
      "default": "./dist/user/seed.js"
    },
    "./material/seed": {
      "types": "./dist/material/seed.d.ts",
      "default": "./dist/material/seed.js"
    },
    "./furnituretype/seed": {
      "types": "./dist/furnituretype/seed.d.ts",
      "default": "./dist/furnituretype/seed.js"
    },
    "./furniture/seed": {
      "types": "./dist/furniture/seed.d.ts",
      "default": "./dist/furniture/seed.js"
    },
    "./image/seed": {
      "types": "./dist/image/seed.d.ts",
      "default": "./dist/image/seed.js"
    },
    "./furniturematerial/seed": {
      "types": "./dist/furniturematerial/seed.d.ts",
      "default": "./dist/furniturematerial/seed.js"
    },
    "./favorite/seed": {
      "types": "./dist/favorite/seed.d.ts",
      "default": "./dist/favorite/seed.js"
    }
  },
  "files": [
    "dist/**",
    "prisma/generated/**",
    "prisma/schema.prisma",
    "prisma/migrations/**",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "commit": "git-cz",
    "prepare": "husky",
    "db:up": "node scripts/db-up.mjs && node scripts/wait-for-mysql.mjs",
    "db:down": "docker compose down -v",
    "dev:db": "sh scripts/dev-db.sh",
    "dev:db:studio": "OPEN_STUDIO=true sh scripts/dev-db.sh",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/main.ts",
    "prisma:seed:dev": "node -e \"process.env.NODE_ENV='development'; require('child_process').spawnSync('tsx',['prisma/main.ts'],{stdio:'inherit'})\"",
    "prisma:seed:soft": "node -e \"process.env.NODE_ENV='production'; process.env.CLEANUP_MODE='soft'; process.env.CONFIRM_SOFT_DELETE='YES_I_AM_SURE'; require('child_process').spawnSync('tsx',['prisma/main.ts'],{stdio:'inherit'})\"",
    "commitlint": "commitlint --edit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "dotenv -e .env.test -- jest",
    "clean": "node -e \"const fs=require('fs');for(const p of ['dist','tsconfig.tsbuildinfo']){fs.rmSync(p,{recursive:true,force:true})}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "npm run clean && npm run typecheck && tsc -p tsconfig.json",
    "prepublishOnly": "npm run prisma:generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:reset:force": "prisma migrate reset --force",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:db:seed": "prisma db seed",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull",
    "prisma:format": "prisma format",
    "db:cleanup": "tsx prisma/helpers/cleanUp.ts",
    "prisma:studio": "prisma studio",
    "publish:package": "npm publish --access public",
    "save:latest": "git add -A && git commit -m \"chore: save latest\" && git push origin HEAD"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ange230700/abyss_db.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Ange230700/abyss_db/issues"
  },
  "homepage": "https://github.com/Ange230700/abyss_db#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.29",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^9.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "validate-branch-name": "^1.3.2"
  },
  "jest": {
    "globalSetup": "./prisma/testSetup.ts",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^abyssdb/(.*)$": "<rootDir>/prisma/$1"
    },
    "collectCoverageFrom": [
      "prisma/**/*.{ts,js}"
    ],
    "coverageDirectory": "./coverage"
  },
  "prisma": {
    "seed": "tsx prisma/main.ts"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|hotfix|release|chore)/.+$",
    "errorMsg": "Branch name must be master, main, develop, feature/xxx, fix/xxx, hotfix/xxx or release/xxx"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "argon2": "^0.43.0"
  },
  "engines": {
    "node": ">=20"
  }
}
